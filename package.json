{
  "name": "hexo-site",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prebuild": "bash ctf_post.sh replace",
    "build": "touch db.json && hexo generate",
    "postbuild": "find public/CTF ! -path '**/images/*' ! -path '**/assets/*' -type f -delete",
    "clean": "touch db.json && hexo clean",
    "deploy": "hexo deploy",
    "server": "hexo server",
    "postinstall":"patch-package"
  },
  "hexo": {
    "version": "6.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "hexo": "^6.3.0",
    "hexo-abbrlink": "^2.2.1",
    "hexo-blog-encrypt": "^3.1.9",
    "hexo-deployer-git": "^4.0.0",
    "hexo-filter-optimize": "^0.3.1",
    "hexo-generator-archive": "^2.0.0",
    "hexo-generator-baidu-sitemap": "^0.1.9",
    "hexo-generator-category": "^2.0.0",
    "hexo-generator-feed": "^3.0.0",
    "hexo-generator-index-custom": "^1.0.1",
    "hexo-generator-json-feed": "^2.0.4",
    "hexo-generator-searchdb": "^1.4.1",
    "hexo-generator-seo-friendly-sitemap": "^0.2.1",
    "hexo-generator-tag": "^2.0.0",
    "hexo-include-markdown": "^1.0.2",
    "hexo-related-popular-posts": "^5.0.1",
    "hexo-renderer-ejs": "^2.0.0",
    "hexo-renderer-marked": "^6.0.0",
    "hexo-renderer-stylus": "^3.0.0",
    "hexo-server": "^3.0.0",
    "hexo-sliding-spoiler": "^1.2.1",
    "hexo-symbols-count-time": "^0.7.1",
    "patch-package": "^8.0.0"
  },
  "devDependencies": {
    "hexo-bridge": "^1.2.0"
  }
}
